[http]
  [http.services]
    [[http.services.jupyterhub.loadBalancer.servers]]
      url = "http://{{ containers.jupyterhub.name }}:8000/"
  [http.routers]
    [http.routers.jupyterhub_https]
      rule = "Host(`jupyterhub.{{ tld }}`)"
      entryPoints = "https"
      service = "jupyterhub"
      middlewares = [
        "security_headers"
      ]
      [http.routers.jupyterhub_https.tls]
        certResolver = "sadm_tlschallenge"
